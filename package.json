{
  "name": "ycpu-assembly",
  "displayName": "yCPU Assembly",
  "description": "Provides basic support for yCPU assembly",
  "publisher": "blackfur",
  "repository": {
    "type": "git",
    "url": "https://github.com/DasBlackfur/ycpu-assembly-vscode"
  },
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ycpu-asm",
        "aliases": [
          "yCPU Assembly",
          "ycpu-asm"
        ],
        "extensions": [
          ".asm",
          ".ysm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ycpu-asm",
        "scopeName": "source.ycpu-asm",
        "path": "./syntaxes/ycpu-asm.tmLanguage.json"
      }
    ]
  }
}
